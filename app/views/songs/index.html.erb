<h3> All Songs </h3>

<table>
  <tr>
    <th> Title  </th>
    <th> Artist Name </th>
    <th> Genre  </th>
    <th> Released  </th>
    <th> Year Released  </th>

  </tr>
  <tbody>
    <% @songs.each do |song| %>
    <tr>
      <td> <%= link_to(song.title, song_path(song)) %> </td>
      <td> <%= link_to(song.artist_name, song_path(song)) %> </td>
      <td> <%= link_to(song.genre, song_path(song)) %> </td>
      <td> <%= link_to(song.released, song_path(song)) %> </td>
      <td> <%= link_to(song.release_year, song_path(song)) %> </td>
    </tr>
    <% end %>
  </tbody>
</table>

<br>
<br>
<div> <%= link_to("Create Song", new_song_path) %> </div>
